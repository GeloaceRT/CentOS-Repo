[centos-openstack-zed]
name=CentOS-$releasever - OpenStack zed
#baseurl=http://mirror.stream.centos.org//8-stream/cloud/$basearch/openstack-zed/
metalink=https://mirrors.centos.org/metalink?repo=centos-cloud-sig-openstack-zed-$releasever-stream&arch=$basearch&protocol=https,http
gpgcheck=1
enabled=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-SIG-Cloud
module_hotfixes=1

[centos-openstack-zed-test]
name=CentOS-$releasever - OpenStack zed Testing
baseurl=https://buildlogs.centos.org/centos/$releasever-stream/cloud/$basearch/openstack-zed/
gpgcheck=1
enabled=0
module_hotfixes=1

[centos-openstack-zed-source]
name=CentOS-$releasever - OpenStack zed - Source
baseurl=http://mirror.stream.centos.org/SIGs/$releasever-stream/cloud/source/openstack-zed/
gpgcheck=1
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-SIG-Cloud
module_hotfixes=1

